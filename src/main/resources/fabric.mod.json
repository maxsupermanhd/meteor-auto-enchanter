{
  "schemaVersion": 1,
  "id": "auto-enchanter",
  "version": "${version}",
  "name": "Auto Enchanter",
  "description": "Automatic item combining on anvil",
  "authors": [
    "FlexCoral"
  ],
  "contact": {
    "repo": "https://github.com/maxsupermanhd/meteor-auto-enchanter"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "maxsuperman.addon.enchanter.EnchanterAddon"
    ]
  },
  "mixins": [
    "template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": ">0.4.5"
  }
}
